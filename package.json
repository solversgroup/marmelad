    "eslint": "eslint **/*.js --ignore-pattern !node_modules/*"
  },
  "jest": {
    "testRegex": "tests/.*\\.(js|jsx)$"
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "fs-extra": "9.0.1",
    "jest": "26.1.0"
