    "ignorePatterns": [
      "node_modules/"
    ],
    "env": {
      "jest": true,
      "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
      "no-console": 0,
      "import/no-dynamic-require": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "global-require": 0,
      "consistent-return": 0,
      "max-len": 0,
      "class-methods-use-this": 0
